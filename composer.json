{
    "name": "bitnoise/media-bundle",
    "type": "symfony-bundle",
    "description": "",
    "keywords": [ "bitnoise", "cms", "media"],
    "homepage": "https://github.com/Bitnoise/BtnMediaBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Bitnoise Team",
            "homepage": "http://www.bitnoi.se/",
            "email": "labs@bitnoi.se"
        }
    ],
    "require": {
        "bitnoise/admin-bundle": "2.0.*",
        "liip/imagine-bundle": "~0.21.0",
        "knplabs/knp-gaufrette-bundle" : "~0.1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.0.0",
        "fabpot/php-cs-fixer": "~0.5.0",
        "phpmd/phpmd": "~2.0.0",
        "sebastian/phpcpd": "~2.0.0",
        "asm89/twig-lint": "~1.0.1"
    },
    "repositories": [
        {
            "type": "composer",
